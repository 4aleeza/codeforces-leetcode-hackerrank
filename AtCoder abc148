/*Asfand is solving quizzes. He has easily solved all but the last one.

The last quiz has three choices: 
1, 2, 3.
Using his intelligence, Asfand has found out that the choices 
A and B are both wrong.
Print the correct choice for this problem.*/


#include <iostream>
using namespace std;
int main() {
	int choice[2];
	for (int i = 0;i < 2;i++) {
		cin >> choice[i];
		if ((choice[i] != 1) && (choice[i] != 2) && (choice[i] != 3)) {
			break;
		}
	}
	if ((choice[0] == 1) && (choice[1] == 2)) {
		cout << "3";
	}
	if ((choice[0] == 2) && (choice[1] == 1)) {
		cout << "3";
	}
	if ((choice[0] == 3) && (choice[1] == 2)) {
		cout << "1";
	}
	if ((choice[0] == 2) && (choice[1] == 3)) {
		cout << "1";
	}
	if ((choice[0] == 3) && (choice[1] == 1)) {
		cout << "2";
	}
	if ((choice[0] == 1) && (choice[1] == 3)) {
		cout << "2";
	}
}
